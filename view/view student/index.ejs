<div class="container">
<div class="well">
<h1 class="text-center">View Student Data</h1>
</div>

<table class="table table-stripped table-bordered">
<tr class="bg-primary">
<th>S.No</th>
<th>STUDENT NAME</th>
<th>AGE</th>
<th>GENDER</th>
<th>HOBBIES</th>
<th>ADDRESS</th>
<th>CITY</th>
<th>DELETE</th>
</tr>

<%data.forEach(function(x,i){%>

<tr>
<td><%=i+1%></td>
<td><%=x.name%></td>
<td><%=x.age%></td>
<td><%=x.gender%></td>
<td><%=x.hobbies%></td>
<td><%=x.address%></td>
<td><%=x.city%></td>
<td><a href="view/remove/<%=x._id%>" class="btn btn-primary" type="submit">DELETE</a></td>
</tr>


<%})%>

</table>
</div>